[Unit]
Description=Elephant walker backend
Documentation=https://github.com/abenz1267/elephant
PartOf=graphical-session.target
After=graphical-session.target

[Service]
Type=simple
ExecStart=/usr/bin/uwsm-app -- elephant
Restart=on-failure
RestartSec=1
TimeoutStopSec=10

[Install]
WantedBy=graphical-session.target
